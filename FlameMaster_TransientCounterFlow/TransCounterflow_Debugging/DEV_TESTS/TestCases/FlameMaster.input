############
# Numerics #
############

MaxIter = 5000
RelTol = 1.0e-6
AbsTol = 1e-9

#### grid ####

initialgridpoints = 203
maxgridpoints = 203
equidistant is true

#######
# I/O #
#######

#PrintMolarFractions is TRUE
OutputPath is ./Outputs
StartProfilesFile is ./CH4_p01a00030tf0300to0300
#_Modified

#############
# Chemistry #
#############

MechanismFile is ../KineticModels/NonReactive/Nonreactive.chmech.pre
globalReaction is CH4 + 2O2 == CO2 + 2H2O;
#TransModel is MonoAtomic

#fuel is H2
fuel is N2
oxidizer is o2

#########
# Flame #
#########

Flame is Transient Counterflow

#ExactBackward is TRUE

pressure = 1.0e5

#ConstantLewisNumber is TRUE
#LewisNumberFile is LewisFile

#ComputeWithRadiation is TRUE
#Thermodiffusion is TRUE

#######################
# Boundary conditions #
#######################

leftbc {
    dirichlet {
#	neumann {
#		V = 0.12
		V = 0.0310075
		t = 300
		Y->N2 = 1.0
	}
}

rightbc {
    dirichlet {
#	neumann {
		t = 300
		V = -0.0758633
#		V = 0
		#X->N2 = 0.79
		Y->O2 = 1.0
	}
}

##################
# Trans1DIsoChor #
##################

TStart			= 0.0								# Startzeit
TEnd			= 500.0e-3							# Dauer in [ms]*10^3
NOutputs		= 100								# Anzahl Outputs pro Zeit

Left			= 0.0								# Nozzle position left
Right			= 16.e-3							# Nozzle position right

DeltaTStart		= 1.00e-6							# Startwert (relativ variabel waehlbar, da direkt angepasst)

#Gamma			= 1								# Exponent alpha in Warnatz-Paper ( = 1 fuer Kreis, = 2 fuer Kugel)
Kappa			= 2								# Groessenverhaeltnis benachbarter Zellen
Tau			= 0.001								# Mass fuer die Schritte die die Rechnung "nachlaeuft" [ 0 < Tau < 1 ]

#EquilibRadius           = 5e-3
#IgnitionEnergy		= 8.0e5
#IgnitionTime		= 1e-4
#IgnitionRadius		= 3e-3

